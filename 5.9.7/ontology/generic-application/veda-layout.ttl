v-s:SemanticMachines
  rdf:type v-s:Bundle ;
  rdfs:label "Смысловые машины"@ru ;
  rdfs:label "Semantic machines"@en ;
.
v-s:License
  rdf:type v-s:Bundle ;
  rdfs:label "Лицензия"@ru ;
  rdfs:label "License"@en ;
.
v-s:WelcomeToVeda
  rdf:type v-s:Bundle ;
  rdfs:label "Добро пожаловать в Veda!"@ru ;
  rdfs:label "Welcome to Veda!"@en ;
.
v-s:VedaDescription
